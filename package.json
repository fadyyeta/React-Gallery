{
	"name": "reactgallery",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "npm run build:css && next build",
		"start": "npm run watch:css && next start",
		"lint": "next lint",
		"build:css": "postcss app/assets/tailwind.css -o app/assets/main.css",
		"watch:css": "postcss app/assets/tailwind.css -o app/assets/main.css"
	},
	"dependencies": {
		"next": "14.2.4",
		"postcss-cli": "^11.0.0",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"autoprefixer": "^10.4.19",
		"eslint": "^8",
		"eslint-config-next": "14.2.4",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.4",
		"typescript": "^5"
	}
}
